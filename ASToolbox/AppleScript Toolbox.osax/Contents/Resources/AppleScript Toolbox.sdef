        <command name="AST guess language for" code="ASTfndln" description="Find the closest possible language of the given string. Typically, the function requires 200-400 characters to reliably guess the language of a string. AST guess language recognizes the following languages: ar (Arabic), bg (Bulgarian), cs (Czech), da (Danish), de (German), el (Greek), en (English), es (Spanish), fi (Finnish), fr (French), he (Hebrew), hr (Croatian), hu (Hungarian), is (Icelandic), it (Italian), ja (Japanese), ko (Korean), nb (Norwegian Bokmål), nl (Dutch), pl (Polish), pt (Portuguese), ro (Romanian), ru (Russian), sk (Slovak), sv (Swedish), th (Thai), tr (Turkish), uk (Ukrainian), zh-Hans (Simplified Chinese), zh-Hant (Traditional Chinese). ">
            <direct-parameter type="text" description="String to examine."/>
            <result>
                <type type="text" list="no"/>
            </result>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST guess language for "Hallo Wereld!"
<em style="color:gray;">&emsp;&#8627;"nl"</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20guess%20language%20for%20%22Hallo%20Wereld%21%22">open in script editor</a></p>]]>
                </html>
            </documentation>
        </command>
        <command name="AST levenshtein distance" code="ASTlevdi" description="Get the levenshtein distance between two strings. The levenshtein distance is a string distance that measures how many single-character edits are needed to turn one string into the other. The single-character edits are delete, insert or edit.">
            <parameter name="by comparing string" type="text" code="|sta" optional="no" description="String A to be compared against &quot;string b&quot;."/>
            <parameter name="against string" type="text" code="|stb" optional="no" description="String B to be compared against &quot;string a&quot;."/>

            <result description="Levenshtein distance.">
                <type type="integer" list="no"/>
            </result>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST levenshtein distance by comparing string "Hello World!" against string "Hallo Wereld!"
<em style="color:gray;">&emsp;&#8627;3</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20levenshtein%20distance%20by%20comparing%20string%20%22Hello%20World%21%22%20against%20string%20%22Hallo%20Wereld%21%22">open in script editor</a></p>]]>
                </html>
            </documentation>

        </command>
        <record-type name="re match record" code="|RER" description="A record containing the matched text, begin offset and end offset of a match.">
