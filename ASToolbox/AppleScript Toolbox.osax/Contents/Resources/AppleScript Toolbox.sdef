    <suite name="Key &amp; Mouse Suite" code="????" description="Automate your mouse">
        <command name="AST click" code="ASTclick" description="Click the mouse at the specified location by sending the events mousedown and mouseup using CoreGraphics. If no coordinate parameter is given, the click is performed at the current mouse point location.">
            <parameter  name="at" code="at  " optional="yes" description="List with the x and y global screen coordination to move the mouse to and perform a mouse down and mouse up event.">
                <type type="real" list="yes" description="The x and y coordinates as real values."/>
                <type type="integer" list="yes" description="The x and y coordinates as integer values."/>
            </parameter>
            <parameter name="number of clicks" code="|nrc" type="integer" optional="yes" description="The number of clicks to perform. (default 1)"/>
            <parameter name="holding modifier key" code="|mod" optional="yes" description="Which modifier key(s) should be down while performing the click.">
                <type type="|MDK" list="yes" />
                <type type="|MDK" list="no" />
            </parameter>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST click at {20,20} -- click the Apple menu
<em style="color:gray;">&emsp;&#8627;</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20click%20at%20%7B20,%2020%7D%20--%20click%20the%20Apple%20menu%0D">open in script editor</a></p>]]>
                </html>
            </documentation>
        </command>
        <command name="AST mouse point location" code="ASTgetlo" description="Get the global location of the mouse cursor using CoreGraphics." >
            <result description="The x and y coordinates as real values.">
                <type type="real" list="yes"/>
            </result>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST mouse point location
<em style="color:gray;">&emsp;&#8627;{20.0, 20.0}</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20mouse%20point%20location">open in script editor</a></p>]]>
                </html>
            </documentation>
