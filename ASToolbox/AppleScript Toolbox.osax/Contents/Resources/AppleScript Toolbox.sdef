            <property name="rm_so" code="|RES" type="integer" access="r" description="Offset in the text of the substring's first character."/>
            <property name="rm_eo" code="|REE" type="integer" access="r" description="Offset in the text of the substring's first character, or 0 when the match was empty."/>
