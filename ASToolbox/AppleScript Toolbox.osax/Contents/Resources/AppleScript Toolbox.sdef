    <suite name="List &amp; Record Suite" code="????" description="Commands that can be applied to lists and records.">
        <command name="AST copy list" code="ASTcplst" description="&quot;Deep copy&quot; with the options to change the list before return.">
            <direct-parameter type="list" description="Original list."/>
            <parameter name="matching regex" code="|rex" type="text" optional="yes" description="The regular expression to match the string items. Items that are not type string (unicode string, text or «class utf8») the item is removed from the list. Enhanced, extended and case insensitive regex options are used. When this parameter is used the parameters 'ommitting item' and 'keeping item' are ignored." />
            <parameter name="keeping item" code="|sav" optional="yes" description="Index or indices of item(s) to keep in the copy. When parameter 'matching regex' is used this parameter is ignored. When this parameter is used parameter 'omitting item' will be ignored.">
                <type type="integer" list="no"/>
                <type type="integer" list="yes"/>
            </parameter>
            <parameter name="omitting item" code="|del" optional="yes" description="Index or indices of item(s) to omit from the copy. When parameter 'matching regex' or 'keeping item' is used this parameter is ignored.">
                <type type="integer" list="no"/>
                <type type="integer" list="yes"/>
            </parameter>
            <parameter name="sort column" code="|soc" type="integer" optional="yes" description="Use this parameter if your list is an table or 2D list. This paramater allows you to sort on a certain field in the sublists. When this parameter is used the sorting items parameter is automatically set to true. Negative indices are not supported." />
            <parameter name="sorting items" code="|srt" type="boolean" optional="yes" description="Sort items to return. For string items you can use the parameters &quot;case sensitivity&quot;, &quot;diacritic sensitivity&quot; or &quot;numeric sorting&quot; to change the sort behaviour." />
            <parameter name="keeping duplicates" type="boolean" code="|dup" optional="yes" description="Should duplicate values be kept in the list? For string items you can use the parameters &quot;case sensitivity&quot; or &quot;diacritic sensitivity&quot; to change the compare behavior.(default is true)"/>
            <parameter name="case sensitivity" type="boolean" code="|cre" optional="yes" description="Should sorting/removing duplicates be case sensitive for string items? (default is false)"/>
            <parameter name="diacritic sensitivity" type="boolean" code="|dso" optional="yes" description="Should sorting/removing duplicates be diacritic sensitive for string items? (default is true)"/>
            <parameter name="numeric sorting" type="boolean" code="|nso" optional="yes" description="Should sorting be numerical sensitive for string items? With this parameter enabled &quot;2&quot; is before &quot;10&quot;. Also when this parameter is enabled integers and real are considered class number and are sorted as a whole. Also when this parameter is set, sorting will be enabled. (default is false)"/>
            <result>
                <type type="any" list="yes"/>
            </result>
        </command>
    </suite>
    <suite name="URL &amp; Network Suite" code="????" description="Commands that are associated with URL and network protocols like URL and data encodings">
        <command name="AST base64 encode" code="AST64enc" description="Encode the given string into an base64 encoded string.">
            <direct-parameter type="text" description="The string to be encoded."/>
            <result description="The string in base64 encoded format">
                <type type="text" list="no"/>
            </result>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST base64 encode "Hello World!"
<em style="color:gray;">&emsp;&#8627;"SGVsbG8gV29ybGQh"</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20base64%20encode%20%22Hello%20World%21%22">open in script editor</a></p>]]>
                </html>
            </documentation>
        </command>
        <command name="AST base64 decode" code="AST64dec" description="Decode the given base64 encoded string into string.">
            <direct-parameter type="text" description="Base64 encoded string to be decoded"/>
            <result description="Decoded string">
                <type type="text" list="no"/>
            </result>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST base64 decode "SGVsbG8gV29ybGQh"
<em style="color:gray;">&emsp;&#8627;"Hello World!"</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20base64%20decode%20%22SGVsbG8gV29ybGQh%22">open in script editor</a></p>]]>
                </html>
            </documentation>
        </command>
        <command name="AST URL encode" code="ASTURLen" description="Encode the given string into an url encoded string. The URL encoding is written according to RFC 3986: chapter 2 (percent escaping).">
            <direct-parameter type="text" description="The string to be encoded."/>
            <parameter name="other space coding" type="boolean" code="|sp+" optional="yes" description="The spaces are not encoded into percent encoding &quot;%20&quot; but in a plus sign &quot;+&quot;. (default false)"/>
            <result description="The string in URL encoded format">
                <type type="text" list="no"/>
            </result>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST URL encode "Hello World!"
<em style="color:gray;">&emsp;&#8627;"Hello%20World%21"</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20URL%20encode%20%22Hello%20World%21%22">open in script editor</a></p>
<p>See also: <a href="http://www.faqs.org/rfcs/rfc3986.html">RFC 3986</a>]]>
                </html>
            </documentation>
        </command>
        <command name="AST URL decode" code="ASTURLde" description="Decode the given URL encoded string into string.">
            <direct-parameter type="text" description="URL encoded string to be decoded"/>
            <parameter name="other space coding" type="boolean" code="|sp+" optional="yes" description="The plus sign is decoded into a space. (default false)"/>
            <result description="Decoded string">
                <type type="text" list="no"/>
            </result>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST URL decode "Hello%20World%21"
<em style="color:gray;">&emsp;&#8627;"Hello World!"</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20URL%20decode%20%22Hello%2520World%2521%22">open in script editor</a></p>
<p>See also: <a href="http://www.faqs.org/rfcs/rfc3986.html">RFC 3986</a>]]>
                </html>
            </documentation>
        </command>
    </suite>
