        <command name="AST set mouse point location" code="ASTmovlo" description="Move the mouse to the specified location using CoreGraphics.">
            <direct-parameter  description="List containing the x and y global screen coordinates to move the mouse to. The values can either be integer or real.">
                <type type="real" list="yes" description="The x and y coordinates as real values."/>
                <type type="integer" list="yes" description="The x and y coordinates as integer values."/>
            </direct-parameter>
            <parameter name="holding mouse down" code="|hmd" type="boolean" description="Whether or not holding mouse down while moving the mouse to another location. (default false)" />
