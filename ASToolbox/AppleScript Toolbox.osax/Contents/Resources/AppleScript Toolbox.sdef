    <suite name="File Suite" code="????" description="File related commands.">
        <command name="AST list folder" code="ASTlstdr" description="Return the file names inside a folder.">
            <direct-parameter type="file specifier" description="Path to folder" />
            <parameter name="matching regex" code="|rex" type="text" optional="yes" description="The regular expression to match the file. Enhanced, extended and case insensitive regex options are used." />
            <parameter name="showing invisibles" code="|hid" type="boolean" optional="yes" description="Should the returned list include hidden items? (default:true)" />
            <parameter name="listing subfolders" code="|rcv" type="boolean" optional="yes" description="List all folders and subfolders? If this parameter is set true the option &quote;returning file specifiers&quote; is also set to true. (default:false)" />
            <parameter name="showing package contents" code="|opp" type="boolean" optional="yes" description="Are packages considered as folders? (default:false)" />
            <parameter name="returning file specifiers" code="|fsp" type="boolean" optional="yes" description="Return the paths to the files as file specifiers? Otherwise only the file names are returned as strings. (default:false)" />
            <parameter name="returning HFS paths" code="|hfs" type="boolean" optional="yes" description="Return the paths to the files as HFS path strings? (default:false)" />
            <parameter name="returning POSIX paths" code="|psx" type="boolean" optional="yes" description="Return the paths to the files as POSIX path strings? (default:false)" />
            <result description="Return a list of file specifiers or list of file names.">
                <type type="file" list="yes"/>
                <type type="text" list="yes"/>
            </result>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST list folder (path to desktop folder) without showing invisibles
<em style="color:gray;">&emsp;&#8627;{"image.jpg", "plain.txt", "table.csv"}</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20list%20folder%20(path%20to%20desktop%20folder)%20without%20showing%20invisibles%0D">open in script editor</a></p>]]>
                </html>
            </documentation>
        </command>
        <command name="AST list volumes" code="ASTlstmp" description="Return the names of the mounted volumes.">
            <parameter name="matching regex" code="|rex" type="text" optional="yes" description="The regular expression to match the volume. Enhanced, extended and case insensitive regex options are used." />
            <parameter name="showing invisibles" code="|hid" type="boolean" optional="yes" description="Should the returned list include hidden items? (default:true)" />
            <parameter name="returning file specifiers" code="|fsp" type="boolean" optional="yes" description="Return the paths to the volumes as file specifiers? Otherwise only the volume names are returned as strings. (default:false)" />
            <parameter name="returning POSIX paths" code="|psx" type="boolean" optional="yes" description="Return the paths to the volumes as POSIX path strings? (default:false)" />
            <parameter name="returning disk info" code="|arb" type="boolean" optional="yes" description="The command returns infomation from the disk arbitration framework and getmntinfo() BSD command. The distinction between disk arbitration and BSD infomation is by the prefix of the property name. Currently BSD only returns the mountFrom path (devices)." />
            <result description="Return a list of file specifiers, list of file names or list of records">
                <type type="file" list="yes"/>
                <type type="text" list="yes"/>
                <type type="record" list="yes"/>
            </result>
            <documentation>
                <html>
                    <![CDATA[<strong>Example code:</strong>
<pre><code style="font-size:10px;">AST list volumes with returning POSIX paths
<em style="color:gray;">&emsp;&#8627;{"/", "/home", "/net"}</em></code></pre>
<p><a href="applescript://com.apple.scripteditor?action=new&script=AST%20list%20volumes%20with%20returning%20POSIX%20paths">open in script editor</a></p>]]>
                </html>
            </documentation>
        </command>
